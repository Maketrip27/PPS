  <section id="contact">
    <div class="container">
      <div class="row">
        <div class="col-sm-12 text-center">
          <h1 class="section-title">Contact</h1>

          <p class="sec-subtitle">Get in Touch with Us!</p>
          </div>
        <div class="col-sm-4" data-sr="enter left, hustle 80px">
          <div class="address">
            <p class="address-title title"><i class="fa fa-map-marker"></i> Address</p>

            <p class="address-content">312, 7th Ave, New York <br>
              NY 101200, United States of America</p>
          </div>
          <div class="address">
            <p class="address-title title"><i class="fa fa-mobile-phone"></i> Hotline (24x7)</p>

            <p class="address-content">+91 9033 857 960, +65 88251 210 <br>
              +88 01723 511 340</p>
          </div>
          <div class="address">
            <p class="address-title title"><i class="fa fa-envelope-o"></i> E-mail</p>

            <p class="address-content">sp98484@gmail.com <br>hello@shapedtheme.com</p>
          </div>
        </div>
        <div class="col-sm-8" data-sr="enter right, hustle 80px">
          <%= form_tag contact_path, method: 'post', class: 'st-form' do %>
            
            <form class="st-form">
              <div class="row">
                <div class="col-sm-6">
                  <div class="form-group ">
                    <%= text_field :text, '', placeholder: 'YOUR NAME', name:'name', class:'form-control', required:'true', tabindex: 1 %>
                  </div>
                </div>
                <div class="col-sm-6">
                  <div class="form-group ">
                    <%= text_field :email, '', placeholder: 'YOUR E-MAIL', name:'email', class:'form-control', required:'true', tabindex: 2, pattern: "[a-zA-Z0-9!#$%&amp;'*+\/=?^_`{|}~.-]+@[a-zA-Z0-9-]+(\.[a-zA-Z0-9-]+)*" %>
                  </div>
                </div>
              </div>
              <div class="row">
                <div class="col-sm-6">
                  <div class="form-group ">
                      <%= text_field :text, '', placeholder: 'YOUR SUBJECT', name:'subject', class:'form-control', required:'true', tabindex: 3 %>
                  </div>
                </div>
                <div class="col-sm-6">
                  <div class="form-group ">
                    <%= text_field :text, '', placeholder: 'COMPANY NAME', name:'company_name', class:'form-control', tabindex: 4 %>
                  </div>
                </div>
              </div>

              <div class="form-group ">
                <%= text_area_tag :message, '', rowa: '4', placeholder: 'WRITE YOUR MESSAGE', name:'message', class:'form-control', required:'true', tabindex: 5 %>
              </div>
              <%= button_tag "SUBMIT", class:'btn btn-send'%>
            <!-- <button type="submit" class="btn btn-send">SUBMIT</button> -->
          <% end %>
        </div>
      </div>
    </div>
  </section>
